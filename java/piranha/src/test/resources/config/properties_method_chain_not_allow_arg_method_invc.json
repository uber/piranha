{
  "methodProperties": [
    {
      "methodName": "[flagName].getValue",
      "flagType": "treated",
      "returnType": "boolean"
    },
    {
      "methodName": "put$[flagName]",
      "flagType": "empty",
      "argumentIndex" : 0
    }
  ],
  "annotations": [
    {
      "name": "BoolParam",
      "flag": "$key",
      "treated": "[notIsTreated]"
    },
    {
      "name": "PVal",
      "flag": "$key",
      "treated": "$val"
    }
  ],
  "cleanupOptions": {
    "allow_method_chain" : true,
    "allow_matching_method_invocation_as_argument" : false
  }
}
